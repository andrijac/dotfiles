startup_message off
silencewait 300

term xterm-256color
hardstatus off
backtick 1 30 1 $HOME/bin/battery.sh
caption always "%?%F%{= wk} * %:%{=u Kk}%? "
hardstatus alwayslastline "%{= kr} %-Lw%{=b wk} %n%f %t %{-}%+Lw %=%{= dd} %{+b ky} %l %{= kw}%1`%{+b kg}%C%a %D %d %M %Y%{= db}"

vbell off

defscrollback 10000

